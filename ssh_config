# ssh config file
#
# Earlier settings take priority over later ones

Host newbase
     HostName %h.openmarket.com

Host 19cw faith dawn giles gilesipmi osmc tara mrpointy
    HostName %h.sw1v.org

Match host *.sw1v.org
     ForwardAgent yes

Match host *.openmarket.com
     User richardv
     ForwardAgent yes

Match host martinbrock.org
     User richard
     ForwardAgent yes

Match host gilesipmi.sw1v.org
     User ADMIN
     ServerAliveInterval 300

Match host giles.sw1v.org,gilesipmi.sw1v.org,tara.sw1v.org
    ProxyCommand ssh 19cw.sw1v.org nc -w 90 %h %p
    ServerAliveInterval 30

Match host osmc.sw1v.org
    User osmc

Host mrpointy.sw1v.org
    User root

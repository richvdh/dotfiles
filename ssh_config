# ssh config file
#
# Earlier settings take priority over later ones

# shortcuts for sw1v stuff
Host 19cw faith dawn
    HostName %h.sw1v.org

# special swiftserve boxes - note these must be before the more general
# *.wip swiftserve shortcut otherwise the HostName is ignored.
Host prov1a.wip.swiftserve.com prov1a.wip
    HostName ec2-52-28-160-121.eu-central-1.compute.amazonaws.com
    Port 22
    User admin
    IdentityFile ~/.ssh/frankfurt-devinfra.pem

Host prov1b.wip.swiftserve.com prov1b.wip
    HostName ec2-52-28-131-248.eu-central-1.compute.amazonaws.com
    Port 22
    User admin
    IdentityFile ~/.ssh/frankfurt-devinfra.pem

